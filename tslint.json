{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": [
      false,
      "log",
      "error"
    ]
  },
  "rulesDirectory": []
}